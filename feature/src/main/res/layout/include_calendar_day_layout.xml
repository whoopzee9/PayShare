<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <View
        android:id="@+id/include_calendar_day_layout__round_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/dp_8" />

    <TextView
        android:id="@+id/include_calendar_day_layout__tv_day"
        style="@style/TextView.Default"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginVertical="@dimen/dp_10"
        android:fontFamily="@font/montserrat_medium"
        android:gravity="center"
        android:includeFontPadding="false"
        android:textColor="@color/black"
        tools:text="22" />

    <FrameLayout
        android:id="@+id/include_calendar_day_layout__round_overlay_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/dp_8">

        <TextView
            android:id="@+id/include_calendar_day_layout__tv_overlay_day"
            style="@style/TextView.Default"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/montserrat_medium"
            android:gravity="center"
            android:includeFontPadding="false"
            android:textColor="@color/black"
            tools:text="22" />
    </FrameLayout>

    <RelativeLayout
        android:id="@+id/include_calendar_day_container_dots"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginBottom="@dimen/dp_10"
        android:gravity="center" >
    </RelativeLayout>
</FrameLayout>